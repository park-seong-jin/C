#include <stdio.h>//Standard Input Output Header
void main(void) {

	int score;
	char grade;
	
	printf("성적을 입력하세요 :");
	scanf("%d",&score);

	if (score > 90) {
		grade = 'A';
	}
	else if (score > 80) {
		grade = 'B';
	}
	else if (score > 70) {
		grade = 'C';
	}
	else if (score > 60) {
		grade = 'D';
	}
	else {
		grade = 'F';
	}

	printf("점수는 %d이고 성적은 %c입니다.", score, grade);
}
